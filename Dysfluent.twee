:: StoryScript [script]
window.Harlowe = { 'Engine': Engine, 'State' : State };

window.setTheme = function(themeName) {
	document.documentElement.setAttribute('theme', themeName)
}

window.setA11y = function(a11yName) {
	document.documentElement.setAttribute('a11y', a11yName)
}

window.getStoragePrefix = function () {
	//return "(Saved Game " + Engine.options.ifid + ") ";
  	return "(Saved Game CAB07C18-1954-43AF-A3B2-5DCAAA38A621) "
}

window.deleteSaveSlot = function (slotName) {
	localStorage.removeItem(getStoragePrefix() + slotName);
}

window.showAch = function(achName) {
	document.getElementsByName(achName)[0].style.display = "block";
}

window.toDone = function(todoName) {
//	if (Harlowe.State.variables[todoName]) { alert(1)};
	document.getElementsByName(todoName)[0].classList.add("strike");
}

window.toDoCheck = function() {
	const AllToDos = ["call", "store", "lunch", "clean", "job"];
	for (let todoName of AllToDos) {
		if (Harlowe.State.variables[todoName]) {
			document.getElementsByName(todoName)[0].classList.add("struck");
        }
    }
}

window.goHome = function() {
	var oHomeTxt = document.getElementsByName('home')[0]
	oHomeTxt.style.color = "#4169E1";
	oHomeTxt.style.cursor = "pointer";
	oHomeTxt.onclick = function(){
		Engine.goToPassage('Title Screen Permanent');
	};
}

