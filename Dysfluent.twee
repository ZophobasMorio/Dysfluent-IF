	background-color: LightGrey;
	color: Black;
}

tw-link {
	 font-weight: normal;
}

tw-link:hover {
    color: inherit;
}

tw-hook {
	 font-weight: normal;
}

tw-hook[style*="colour:"] tw-link, tw-hook[style*="colour:"] .enchantment-link {
	color: inherit;
}

.visited:hover {
    color: inherit;
}

tw-hook[name="ach1"] {
	display: none;
	animation: fade-out 2s 15s forwards;
}

tw-hook[name="ach2"] {
	display: none;
	animation: fade-out 2s 15s forwards;
}

tw-hook[name="achtxt1"] {
	display: block;
	line-height: 0.9;
}

tw-hook[name="achtxt2"] {
	display: block;
	line-height: 0.9;
}

tw-hook[name="achbox"] {
	background-color: white;
	display: flex;
	justify-content: center; /* Align horizontal */
	align-items: center; /* Align vertical */
	margin-left: 20px;
	padding: 20px;
	width: 200px;
	height: 100px;
	border: solid 1px black;
	animation: slide-in 2s ease-out, fade-out 2s 15s forwards;
}

@keyframes slide-in {
	from {
		height: 0px;
		font-size: 1pt;
		padding: 0px;
		transform: translateX(-250px);
	}
}

@keyframes fade-out {
	to {
		opacity: 0%;
		height: 0px;
		font-size: 1pt;
		padding: 0px;
		display: none;
	}
}


tw-story[tags~="todont"] tw-hook[name="todo"] {
	display: none;
}

tw-hook[name="todo"] {
	background-color: LightYellow;
	display: flex;
	text-align: left; /* Align left */
	margin-left: 20px;
	padding: 20px;
	width: 200px;
	border: solid 1px black;
}

tw-hook[name="todo"] ul {
	margin-block: 2px;
	margin-inline: 0px;
	padding-inline-start: 10px; 
}

.strike::after {
	content: ' ';
	position: absolute;
	top: 45%;
	left: 0;
	width: 100%;
	height: 3px;
	background: dimgray;
	animation: strike 1s forwards; 
}

@keyframes strike{
  0%   { width : 0; }
  100% { width: 100%; }
}


.struck::after {
	content: ' ';
	position: absolute;
	top: 45%;
	left: 0;
	width: 100%;
	height: 3px;
	background: dimgray;
}